{
    "name": "thalfm/laravel-eloquent-filter",
    "description": "Allows filtering Eloquent queries by input filters",
    "require": {
        "laravel/framework": "6.*"
    },
    "keywords": [
        "laravel",
        "eloquent",
        "filter",
        "input",
        "request"
    ],    
    "license": "MIT",
    "authors": [
        {
            "name": "Thales Farias Martins",
            "email": "thalfm@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Thalfm\\LaravelEloquentFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thalfm\\LaravelEloquentFilter\\Tests\\": "tests"
        }
    },    
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "mockery/mockery": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.5",
        "php-coveralls/php-coveralls": "^2.0"
    }
}
